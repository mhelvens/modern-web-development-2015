# top-level EditorConfig file
root = true

[*]
end_of_line = lf
charset = utf-8

[*.js]
indent_style = tab
trim_trailing_whitespace = true
insert_final_newline = true

[*.json]
indent_style = tab
trim_trailing_whitespace = true
insert_final_newline = true

[*.scss]
indent_style = tab
trim_trailing_whitespace = true
insert_final_newline = true

[*.html]
trim_trailing_whitespace = true
insert_final_newline = true
